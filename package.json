    "is-domain-name": "~1.0.1",
    "path-sort": "~0.1.0",
    "restore-cursor": "~3.1.0",
    "run-parallel": "~1.1.9",
    "run-series": "~1.1.8",
    "run-waterfall": "~1.1.6",
    "sha": "~3.0.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.641.0",
    "eslint": "^7.1.0",
    "husky": "^4.2.5",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.2",
